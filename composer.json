{
  "name": "web-composer/synchronization-bundle",
  "description": "read a php composer project and save informations in an database",
  "keywords": [
    "package synchronizer"
  ],
  "homepage": "https://github.com/Seretos/WebComposerSynchronizationBundle/",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Arne von Appen",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.9|7.0.*",
    "symfony/symfony": "3.1.*",
    "doctrine/orm": "2.5.*",
    "jms/composer-deps-analyzer": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.6.*",
    "symfony/phpunit-bridge": "3.1.*",
    "doctrine/doctrine-fixtures-bundle": "2.3.*"
  },
  "autoload": {
    "psr-4": {
      "WebComposer\\SynchronizationBundle\\": ""
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Seretos/WebComposerSynchronizationBundle"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
